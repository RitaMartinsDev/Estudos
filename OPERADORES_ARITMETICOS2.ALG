algoritmo "Operadores_Aritmeticos"
var

n1, n2, total: inteiro

inicio

Escreva ("Digite um número: ")
Leia(n1)
ESCREVA ( "Digite outro número: ")
Leia(n2)
total <- n1 + n2
 ESCREVAL ("soma = ",  total)
total <- n1 - n2
 ESCREVAL ("subtracao = ",  total)
total <- n1 * n2
 ESCREVAL ("multiplicacao =",  total)



fimalgoritmo