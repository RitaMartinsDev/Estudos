algoritmo "Operadores_Aritmeticos_Reais"
var

n1, n2, total: real

inicio

Escreva ("Digite um número: ")
Leia(n1)
ESCREVA ( "Digite outro número: ")
Leia (n2)


total <- n1 + n2
 ESCREVAL ("soma = ",  total)
total <- n1 - n2
 ESCREVAL ("subtracao = ",  total)
total <- n1 * n2
 ESCREVAL ("multiplicacao =",  total)
 total <- n1 / n2
 Escreva (" Dvisao = ", total)



fimalgoritmo