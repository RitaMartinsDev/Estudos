algoritmo "Operadores_Aritmeticos"
var

n1, n2, soma, subtracao, multiplicacao : inteiro

inicio

Escreva ("Digite um número: ")
Leia(n1)
ESCREVA ( "Digite outro número: ")
Leia(n2)
soma <- n1 + n2
subtracao <- n1 - n2
multiplicacao <- n1 * n2
ESCREVAL ("soma = ",  Soma)
ESCREVAL ("subtracao = ",  Subtracao)
ESCREVAL ("multiplicacao =",  Multiplicacao)

fimalgoritmo